float xSpeed = 1;
float x = 0;
float count = 0;

float averageX = (329.0834813990112 + 484.23721839874435 + 381.720536443452) / 3.0;
float averageY = (425.5837660709129 + 396.5645521326987 + 276.4336035236624) / 3.0;

void draw() 
{

  if (x >= 120) {
    stop();
    println(count);
  }

  count++;
  x += xSpeed;

  pg.beginDraw();

  pg.fill(0);
  pg.noStroke();
  pg.background(0, 0);

  pg.ellipseMode(CENTER);

  pg.translate(averageX, averageY);

  pg.scale(0.8);

  pg.rotate(radians(x));

  pg.translate(-averageX, -averageY);

  for (Circle circle : circles) {
    circle.animate(pg);
  }

  pg.endDraw();
  String str = count + "_iota" + ".png";
  pg.save(str);

  background(255);
  image(pg, 0, 0);
}
